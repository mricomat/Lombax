{
  "name": "Lombax",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest --coverage --silent",
    "lint": "eslint \"src/**/*.ts\" \"src/**/*.tsx\"",
    "version-major": "rnsemup -b -u major",
    "version-minor": "rnsemup -b -u minor",
    "version-patch": "rnsemup -b -u patch",
    "format": "prettier-eslint fix --write --eslint-config-path ./.eslintrc.json \"$PWD/src/**/*.ts\" \"$PWD/src/**/*.tsx\"",
    "format-soft": "prettier-eslint check --eslint-config-path --list-different $PWD/.eslintrc.json \"$PWD/src/**/*.ts\" \"$PWD/src/**/*.tsx\"",
    "prepare-commit": "yarn format && yarn lint",
    "test-output": "open coverage/lcov-report/index.html",
    "test-and-output": "yarn test && yarn test-output",
    "e2e-build": "detox build",
    "e2e-run": "detox test",
    "e2e": "yarn e2e-build && yarn e2e-run",
    "clean-metro": "watchman watch-del-all && rm -rf /tmp/metro-bundler-cache-* && rm -rf /tmp/haste-map-react-native-packager-*",
    "lint-out": "yarn lint > lint.txt",
    "reinstall-node": "rm -r node_modules/ && yarn",
    "check": "npm audit",
    "fix": "npm audit fix",
    "android-release": "react-native run-android --variant=release",
    "adb": "adb reverse tcp:8081 tcp:8081",
    "apk": "cd ./android ./gradlew assembleRelease && cd ..",
    "apk-install": "adb install /Users/jdominguezr/Documents/clubUs-app/android/app/build/outputs/apk/release/app-release.apk",
    "apk-build-install": "yarn apk && yarn apk-install",
    "aab": "cd ./android ./gradlew bundleRelease && cd ..",
    "apk-aab": "yarn apk && yarn aab",
    "log-android": "react-native log-android",
    "ios-se": "react-native run-ios --simulator=\"iPhone SE\"",
    "pod": "cd ios && pod install",
    "xcode": "open ./ios/Lombax.xcworkspace",
    "studio": "studio ./android"
  },
  "dependencies": {
    "@ptomasroos/react-native-multi-slider": "^2.2.2",
    "@react-native-community/async-storage": "^2.0.0-rc.0",
    "@react-native-community/checkbox": "^0.5.6",
    "@react-native-community/hooks": "^2.6.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/bottom-tabs": "^5.11.2",
    "@react-navigation/native": "^5.8.10",
    "@react-navigation/stack": "^5.12.8",
    "@testing-library/react-hooks": "^3.4.2",
    "@types/react-native-snap-carousel": "^3.8.2",
    "axios": "^0.21.0",
    "class-validator": "^0.12.2",
    "date-fns": "^2.16.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "prettier-eslint-cli": "^5.0.0",
    "react": "16.13.1",
    "react-native": "0.63.3",
    "react-native-bootsplash": "^3.1.2",
    "react-native-config": "^1.4.1",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-indicators": "^0.17.0",
    "react-native-keyboard-manager": "^4.0.13-17",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-modal": "^11.5.6",
    "react-native-reanimated": "^1.13.2",
    "react-native-root-siblings": "^4.0.6",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.15.0",
    "react-native-semver-update": "^1.1.2",
    "react-native-skeleton-content-nonexpo": "^1.0.11",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-svg": "^12.1.0",
    "react-native-swiper": "^1.6.0",
    "react-native-version-number": "^0.3.6",
    "react-native-video": "^5.1.0-alpha8",
    "react-native-youtube": "^2.0.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/jest-native": "^3.4.3",
    "@testing-library/react-native": "^7.1.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@types/react-native": "^0.63.37",
    "@types/react-test-renderer": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "babel-jest": "^26.6.3",
    "detox": "^17.13.2",
    "eslint": "^7.14.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "prettier": "^2.2.0",
    "react-hooks-testing-library": "^0.6.0",
    "react-native-rename": "^2.5.0",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-typescript-transformer": "^1.2.13",
    "react-test-renderer": "^17.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": ""
    }
  }
}
